<main class="main-watches" data-urltype="<%= data.urlType %>">
  <!-- <div class="frontend-section window">
    <h1>ÓRÁK</h1>
  </div> -->
  <div class="frontend-section row justify-content-center">
    <div class="col-md-3">
      <div class="filters" id="filters">
      </div>
      <div class="price">
        <div class="d-flex justify-content-between flex-wrap mb-3">
          <div>Ár:</div> <div id="priceRange"></div>
        </div>

      	<input type="hidden" id="minPrice">
        <input type="hidden" id="maxPrice">
        <div id="slider-range" class="ui-slider ui-corner-all ui-slider-horizontal ui-widget ui-widget-content">
          <div class="ui-slider-range ui-corner-all ui-widget-header" style="left: 34%;">
          </div>
          <span tabindex="0" class="ui-slider-handle ui-corner-all ui-state-default" style="left: 34%;"></span>
          <span tabindex="0" class="ui-slider-handle ui-corner-all ui-state-default" style="left: 61.4%;"></span>
        </div>
      </div>
    </div>
    <div class="col-md-9 watches">
      <div class="watches-header d-flex justify-content-between">
        <div class="">
          <select class="form-control" id="order">
            <option value="best-match">&#xf091;&nbsp;&nbsp;&nbsp;Legjobb találat</option>
            <option value="name-asc">&#xf15d;&nbsp;&nbsp;&nbsp;Név szerint</option>
            <option value="price-asc">&#xf160;&nbsp;&nbsp;&nbsp;Növekvő árak</option>
            <option value="price-desc">&#xf161;&nbsp;&nbsp;&nbsp;Csökkenő árak</option>
          </select>
        </div>
        <div class="d-flex align-items-center">
          <span id="matches"></span>&nbsp;találat
        </div>
      </div>
      <div class="row"  id="mainWatches">

      </div>
    </div>
  </div>
</main>
