<%
    view.current = {}
    if(view.super.from_controller.lang) {
        view.current.lang = view.super.from_controller.lang
    } else view.current.lang = view.config.lang ? view.config.lang : view.super.main.lang

    if(view.super.from_controller.title) {
        view.current.title = view.super.from_controller.title
    } else view.current.title =  view.config.title ? view.config.title : view.super.main.title

    if(view.super.from_controller.meta) {
        view.current.meta = view.super.from_controller.meta
    } else view.current.meta = view.config.meta ? view.config.meta : view.super.meta

    if(view.super.from_controller.cards) {
        view.current.cards = view.super.from_controller.cards
    } else view.current.cards = view.config.cards ? view.config.cards : view.super.cards

    if(view.super.from_controller.og) {
        view.current.og = view.super.from_controller.og
    } else view.current.og = view.config.og ? view.config.og : view.super.og

    if(view.super.from_controller.files) {
        view.current.files = view.super.from_controller.files
    } else view.current.files = view.config.files ? view.config.files : {}

    view.current.files.headers = view.current.files.headers ? view.current.files.headers : []
    view.current.files.footers = view.current.files.footers ? view.current.files.footers : []
    view.current.files.filesToInclude = view.current.files.filesToInclude ? view.current.files.filesToInclude : []
    view.current.files.mainFiles = view.current.files.mainFiles ? view.current.files.mainFiles : {}

    if(view.super.from_controller.css) {
        view.current.css = view.super.from_controller.css
    } else view.current.css = view.config.css ? view.config.css : []

    if(view.super.from_controller.js) {
        view.current.js = view.super.from_controller.js
    } else view.current.js = view.config.js ? view.config.js : []

    if(view.super.from_controller.fonts) {
        view.current.fonts = view.super.from_controller.fonts
    } else view.current.fonts = view.config.fonts ? view.config.fonts : []

    if(view.super.from_controller.mainJSON) {
        view.current.mainJSON = view.super.from_controller.mainJSON == "off" ? false : true
    } else view.current.mainJSON = view.config.mainJSON == "off" ? false : true

    if(view.super.from_controller.mainFiles) {
        view.current.mainFiles = view.super.from_controller.mainFiles == "off" ? false : true
    } else view.current.mainFiles = view.config.mainFiles == "off" ? false : true

    if(view.super.from_controller.mainHeader) {
        view.current.mainHeader = view.super.from_controller.mainHeader == "off" ? false : true
    } else view.current.mainHeader = view.config.mainHeader == "off" ? false : true

    if(view.super.from_controller.mainFooter) {
        view.current.mainFooter = view.super.from_controller.mainFooter == "off" ? false : true
    } else view.current.mainFooter = view.config.mainFooter == "off" ? false : true

    view.super = null
    view.config = null
%><%
if(view.current.mainFiles) { %>
<%- view.current.files.mainFiles.head ? include("includes/heads/" + view.current.files.mainFiles.head + ".ejs") : include('includes/main/head.ejs') %> <% }%>
<%- include('Views/' + view.folder + '/main.ejs') %><%
if(view.current.mainFiles) { %>
<%- view.current.files.mainFiles.foot ? include("includes/foots/" + view.current.files.mainFiles.foot + ".ejs") : include('includes/main/foot.ejs') %> <% }%>
